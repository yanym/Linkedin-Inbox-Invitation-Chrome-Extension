{
  "manifest_version": 2,
  "name": "Linkedin Message Browser",
  "short_name": "Linkedin MSG",
  "description": "Check your Linkedin Messages",
  "default_locale": "en",
  "version": "1.0",
  "author": "Ed",
  "browser_action": {
    "default_icon": "./assets/icon.png",
    "default_popup": "./src/popup.html",
    "default_title": "LinkedIn MSG"
  },
  "permissions": [
    "cookies",
    "*://*.linkedin.com/"
  ],
  "icons": {
    "200": "./assets/icon.png"
  }
}
